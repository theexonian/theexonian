{
	"name": "Shortcodes",
	"version": "1.0",
	"description": "Built-in shortcodes",
	"demo": "http://koken.me",
	"author": {
		"name": "Koken",
		"link": "http://koken.me"
	},

	"oembeds": {
		"twitter": {
			"title": "Twitter",
			"endpoint": "https://api.twitter.com/1/statuses/oembed.json",
			"regex": "https?://(www\\.)?twitter.com/.*/status(es)?/\\d+",
			"icon": "data:image/png;base64,"
		},
		"vimeo": {
			"title": "Vimeo",
			"endpoint": "http://vimeo.com/api/oembed.json",
			"regex": "https?://(www\\.)?vimeo.com/.*",
			"icon": "data:image/png;base64,"
		},
		"flickr": {
			"title": "Flickr",
			"endpoint": "http://www.flickr.com/services/oembed/?format=json",
			"regex": "https?://(www\\.)?flickr.com/photos/.*/\\d+(/in/.*)?/?",
			"icon": "data:image/png;base64,"
		},
		"hulu": {
			"title": "Hulu",
			"endpoint": "http://www.hulu.com/api/oembed.json",
			"regex": "https?://(www\\.)?hulu.com/watch/\\d+/.*",
			"icon": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABkAAAAZCAYAAADE6YVjAAAAoUlEQVR42u3UQQ2EMBCFYSQgAQmVUAlcel8JlVAHSKiElYAEJKyEShgeySQkZCC8JuyFHv7LkOELaUMnIo/XkIb8GQkhuK2TZyP6IdEmFFFikYQKGg/zXudi9K1A9mXkde4tQGHHIh4V40XLCTLrLv8lRFMNMqBCIB8aUSjeBIruVCHuJhI5xIbmCyAbe9Ttiihzh80j2brGOh/aD7IhL0JWQLm9CjCSNZgAAAAASUVORK5CYII="
		},
		"youtube": {
			"title": "YouTube",
			"endpoint": "http://www.youtube.com/oembed?format=json",
			"regex": "https?://(www\\.)?(youtu.be|youtube.com/watch).*",
			"icon": "data:image/png;base64,"
		},
		"instagram": {
			"title": "Instagram",
			"endpoint": "http://api.instagram.com/oembed",
			"regex": "http://(www\\.)?(instagr.am|instagram.com)/p/.*",
			"icon": "data:image/png;base64,"
		},
		"soundcloud": {
			"title": "SoundCloud",
			"endpoint": "http://soundcloud.com/oembed?format=json",
			"regex": "https?://(www\\.)?(snd.sc|soundcloud.com)/.*",
			"icon": "data:image/png;base64,"
		}
	},

