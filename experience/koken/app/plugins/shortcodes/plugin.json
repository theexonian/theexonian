					"type": "string",
					"placeholder": "Enter an image caption"
				},
				"link": {
					"label": "Link",
					"type": "string",
					"placeholder": "http://koken.me",
					"validation": {
						"type": "url",
						"allow_empty": true
					}
				},
				"target": {
					"label": "Link target",
					"type": "select",
					"options": [
						{ "label": "Same window", "value": "none" },
						{ "label": "New window", "value": "_blank" }
					]
				}
			}

		},

		"koken_code": {

			"icon": "data:image/png;base64,",
			"title": "Embed custom code",
			"description": "Insert a custom snippet of code into this entry",

			"data": {
				"code": {
					"type": "code",
					"placeholder": "Insert HTML/JS code here."
				}
			}

		}


	}
